{
  "name": "@mvarble/gatsby-plugin-mdx-reference",
  "author": "Matthew Varble",
  "license": "MIT",
  "scripts": {
    "build": "npx babel src --out-dir dist/ --ignore \"**/__tests__\" --ignore \"**/__mocks__\" --extensions \".ts\"",
    "typegen": "npx tsc --emitDeclarationOnly --declaration --declarationDir dist/",
    "prepare": "NODE_ENV=production npm run build && npm run typegen",
    "watch": "npx babel -w src --out-dir dist/ --ignore \"**/__tests__\" --extensions \".ts,.tsx\"",
    "test": "npx jest"
  },
  "peerDependencies": {
    "gatsby": "4.18.0-mdxv4-rc.65",
    "gatsby-plugin-mdx": "4.0.0-rc.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "cross-env": "^7.0.3",
    "typescript": "^4.7.4"
  }
}
